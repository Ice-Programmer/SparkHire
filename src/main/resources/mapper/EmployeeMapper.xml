<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ice.sparkhire.mapper.EmployeeMapper">

    <resultMap id="BaseResultMap" type="com.ice.sparkhire.model.entity.Employee">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="userId" column="user_id" jdbcType="BIGINT"/>
        <result property="age" column="age" jdbcType="INTEGER"/>
        <result property="qualifications" column="qualifications" jdbcType="VARCHAR"/>
        <result property="education" column="education" jdbcType="INTEGER"/>
        <result property="graduationYear" column="graduation_year" jdbcType="INTEGER"/>
        <result property="jobStatus" column="job_status" jdbcType="TINYINT"/>
        <result property="cityId" column="city_id" jdbcType="BIGINT"/>
        <result property="latitude" column="latitude" jdbcType="DECIMAL"/>
        <result property="longitude" column="longitude" jdbcType="DECIMAL"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
        <result property="isDelete" column="is_delete" jdbcType="TINYINT"/>
    </resultMap>
</mapper>
